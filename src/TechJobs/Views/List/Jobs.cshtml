<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@foreach (Dictionary<string, string> row in ViewBag.jobs)
{
    <table class="job-listing">
        <tbody>
            @foreach (KeyValuePair<string, string> kvp in row)
            {


                <tr>
                    <td>
                        @kvp.Key
                    </td>
                    <td>
                        @kvp.Value
                    </td>

                </tr>

            }
        </tbody>
    </table>
}
